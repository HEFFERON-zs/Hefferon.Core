    <PackageProjectUrl>https://github.com/HEFFERON-zs/Hefferon.Core</PackageProjectUrl>
    <RepositoryUrl>https://github.com/HEFFERON-zs/Hefferon.Core</RepositoryUrl>
    <Authors>Dalibor Vaněk</Authors>
    <Company>Hefferon z.s.</Company>
